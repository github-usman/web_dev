<%-include('./partials/header')%>
<div class="body_blog">
<div class="container">
  <h1>Home</h1>
  <p><%= homeContent %></p> 
</div>
<p><%#= {posts} %></p> 

<% posts.forEach( (Element)=>{ %>
 <h1><%= Element.title %></h1>
<% if((Element.message).length>100){%>
 <p><%= (Element.message).substring(0,100)+"..." %> <a href="posts/<%=Element.title%>">Read More</a></p>
 <%}else{%>
  <p><%= Element.message%></p>
    <%}%>
<%});%>
</div>
<%- include('./partials/footer')%>


